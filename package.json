{
  "name": "webpack-sandboxed",
  "version": "0.1.0",
  "description": "Webpack in a sandbox.",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "repository": "https://github.com/mfellner/webpack-sandboxed.git",
  "license": "MIT",
  "keywords": [
    "webpack"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "npm run clean && babel lib -d dist",
    "test": "NODE_ENV=test jest --coverage",
    "flow-check": "flow check",
    "flow-typed-install": "rimraf flow-typed/npm/*_vx.x.x.js && flow-typed install -o",
    "prepublish": "npm run clean && npm run dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "peerDependencies": {
    "debug": "^2.2.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "memory-fs": "0.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-es2015-destructuring": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-preset-latest-minimal": "1.1.2",
    "flow-bin": "0.38.0",
    "flow-typed": "2.0.0",
    "jest": "18.1.0",
    "rimraf": "2.5.4",
    "webpack": "2.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ]
  }
}
